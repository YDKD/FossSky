{
  "name": "foss-sky",
  "version": "0.0.0",
  "scripts": {
    "dev": "cross-env vite",
    "build": "cross-env vue-tsc --noEmit && vite build",
    "build-dev": "cross-env vue-tsc --noEmit && vite build --mode dev",
    "build-release": "cross-env vue-tsc --noEmit && vite build --mode release",
    "lint": "eslint --fix --ext .js,.jsx,.vue,.ts src/",
    "preview": "vite preview",
    "prettier": "prettier --write .",
    "prepare": "husky install",
    "commit": "cz"
  },
  "dependencies": {
    "axios": "^0.25.0",
    "element-plus": "^1.3.0-beta.8",
    "less": "^4.1.2",
    "normalize.css": "^8.0.1",
    "pinia": "^2.0.9",
    "vue": "^3.2.25",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/node": "^16.11.10",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@vitejs/plugin-vue": "^2.0.0",
    "autoprefixer": "^10.4.2",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^14.3.2",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.3.0",
    "husky": "^7.0.0",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.19",
    "typescript": "^4.5.2",
    "unplugin-auto-import": "^0.5.11",
    "unplugin-element-plus": "^0.2.0",
    "unplugin-vue-components": "^0.17.18",
    "vite": "^2.7.2",
    "vue-eslint-parser": "^8.1.0",
    "vue-tsc": "^0.29.8"
  }
}
